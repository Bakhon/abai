        ]
      }
    },
    {
      "title": "year_fact_cits",
      "code": "year_fact_cits",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "year_fact_gs",
      "code": "year_fact_gs",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "year_deviation",
      "code": "year_deviation",
      "type": "calc",
