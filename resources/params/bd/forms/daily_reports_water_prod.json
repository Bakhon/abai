{
  "type": "table",
  "table_type": "wells",
  "filter": [
    {
      "code": "date",
      "title": "date",
      "type": "date",
      "default": "today"
    },
    {
      "code": "type",
      "type": "list",
      "default": "ЦИТС",
      "values": [
        "ЦИТС",
        "ГС",
        "ЦИТС/ГС"
      ],
      "title": ""
    }
  ],
  "columns": [
    {
      "title": "id",
      "code": "id",
      "type": "integer",
      "visible": false,
      "is_editable": false
    },
    {
      "title": "org_name",
      "code": "org_name",
      "type": "float",
      "is_editable": false
    },
    {
      "title": "plan",
      "code": "plan",
      "type": "float",
      "is_editable": false,
      "validation": "numeric",
      "validate_deviation": true
    },
    {
      "title": "fact",
      "code": "fact",
      "type": "float",
      "is_editable": true,
      "validation": "numeric",
      "validate_deviation": true,
      "show_if": {
        "type": [
          "ЦИТС",
          "ГС"
        ]
      }
    },
    {
      "title": "daily_fact_cits",
      "code": "daily_fact_cits",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "daily_fact_gs",
      "code": "daily_fact_gs",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "daily_deviation",
      "code": "daily_deviation",
      "type": "calc",
      "formula": "$fact$ - $plan$",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС",
          "ГС"
        ]
      }
    },
    {
      "title": "cits_gs_daily_deviation",
      "code": "cits_gs_daily_deviation",
      "type": "calc",
      "formula": "Math.round(($daily_fact_cits$ - $daily_fact_gs$) * 100)/100",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "month_plan",
      "code": "month_plan",
      "type": "float",
      "is_editable": false
    },
    {
      "title": "month_fact",
      "code": "month_fact",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС",
          "ГС"
        ]
      }
    },
    {
      "title": "month_fact_cits",
      "code": "month_fact_cits",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "month_fact_gs",
      "code": "month_fact_gs",
      "type": "float",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "month_deviation",
      "code": "month_deviation",
      "type": "calc",
      "formula": "Math.round(($month_fact$ - $month_plan$) * 100)/100",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС",
          "ГС"
        ]
      }
    },
    {
      "title": "cits_gs_month_deviation",
      "code": "cits_gs_month_deviation",
      "type": "calc",
      "formula": "$month_fact_cits$ - $month_fact_gs$",
      "is_editable": false,
      "show_if": {
        "type": [
          "ЦИТС/ГС"
        ]
      }
    },
    {
      "title": "year_plan",
      "code": "year_plan",
